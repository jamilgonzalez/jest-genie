{
  "name": "jest-genie",
  "publisher": "jamilgonzalez",
  "displayName": "Jest Genie",
  "icon": "./jest-genie.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamilgonzalez/jest-genie"},
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:myExtensionExplorer",
    "onView:myExtensionExplorer"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "editor/title": [
        {
          "command": "tests-generator-poc.generateTests",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "tests-generator-poc.generateTests",
          "when": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == typescriptreact",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "tests-generator-poc.generateTests",
        "title": "Jest Genie: Generate Tests"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "start": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "delay": "^5.0.0",
    "openai": "^3.2.1"
  }
}
